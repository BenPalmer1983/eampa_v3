! Pair PD PD
START
FILE              pd_pair_analytic.pot
FIT               pd_pair.fit
LABEL             PD   PD
F_ON true
F_TYPE            PAIR            ! PAIR DENS EMBE
F_GROUP
R_CUT              6.5
ZOOR     true  ! zero out of range
END


! PD Density
START
FILE              pd_dens_analytic.pot
FIT               pd_dens.fit
LABEL             PD
F_ON true
F_TYPE            DENS            ! PAIR DENS EMBE
F_GROUP           1
R_CUT             6.5
END


! PD Embedding
START
FILE              pd_embe_analytic.pot
FIT               pd_embe.fit
LABEL             PD
F_ON true
F_TYPE            EMBE            ! PAIR DENS EMBE
F_GROUP           1
R_CUT              6.5
ZOOR     true  ! zero out of range
END




