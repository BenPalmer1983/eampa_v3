! FE Density
START
FILE              fe_dens_analytic.pot
FIT               fe_dens.fit
LABEL             FE
F_ON true
F_TYPE            DENS            ! PAIR DENS EMBE
F_GROUP           Group1
R_CUT             6.5
END


! FE Embedding
START
FILE              fe_embe_analytic.pot
FIT               fe_embe.fit
LABEL             FE
F_ON true
F_TYPE            EMBE            ! PAIR DENS EMBE
F_GROUP           Group1
R_CUT              6.5
ZOOR     true  ! zero out of range
END




